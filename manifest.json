{
  "manifest_version": 2,

  "name": "TransNow",
  "description": "This extension translate itens in real time in the page",
  "version": "0.7",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "http://www.alzaro.com.br:8080/",
    "contextMenus"
  ],
  "icons":{
    "16":"icon.png"
  },
  "background":{
    "scripts" : ["doTranslations.js"],
    "persistent": false
  }
}
